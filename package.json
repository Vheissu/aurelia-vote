{
  "name": "aurelia-vote",
  "version": "1.0.0",
  "private": true,
  "author": "Dwayne Charrington <dwaynecharrington@gmail.com> (https://ilikekillnerds.com)",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Vheissu/aurelia-vote.git"
  },
  "bugs": {
    "url": "https://github.com/Vheissu/aurelia-vote/issues"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "clean": "npm cache clean && del-cli node_modules \"test/coverage\" dist",
    "clean:dist": "del-cli dist",
    "clean:install": "npm install",
    "clean:start": "npm start",
    "package:normalize": "cross-env ./node_modules/.bin/fixpack",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "pretest": "del-cli \"test/coverage\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --inline --progress --profile --watch",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "http-server dist --cors",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.tsnode.json TS_NODE_FAST=true ts-node ./node_modules/karma/bin/karma start test/karma.conf.js",
    "test:debug": "npm test -- --single-run=false --debug",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "webdriver:start": "cross-env ./node_modules/.bin/webdriver-manager start",
    "webdriver:update": "cross-env ./node_modules/.bin/webdriver-manager update",
    "webpack": "cross-env TS_NODE_PROJECT=tsconfig.tsnode.json TS_NODE_FAST=true ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env TS_NODE_PROJECT=tsconfig.tsnode.json TS_NODE_FAST=true ./node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "aurelia-bootstrapper": "^2.0.1",
    "aurelia-fetch-client": "^1.1.1",
    "aurelia-loader-webpack": "^2.1.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.3",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "morgan": "^1.8.1",
    "serve-favicon": "^2.4.1",
    "swig": "^1.4.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "@types/webpack": "^2.2.7",
    "aurelia-webpack-plugin": "^2.0.0-rc.1",
    "autoprefixer": "^6.7.6",
    "awesome-typescript-loader": "^3.1.2",
    "bundle-loader": "^0.5.4",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "del-cli": "^0.2.0",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "fixpack": "^2.3.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.7.1",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "node-sass": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "precss": "^1.4.0",
    "sass-loader": "^6.0.2",
    "semantic-release": "^6.3.2",
    "source-map-loader": "^0.2.0",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "style-loader": "^0.13.1",
    "ts-node": "^2.0.0",
    "tslib": "^1.6.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.7",
    "wait-on": "^2.0.1",
    "wallaby-webpack": "^0.0.34",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
